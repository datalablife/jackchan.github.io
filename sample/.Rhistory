# Chunk 1: setup
knitr::opts_chunk$set(
echo = TRUE,
tidy = TRUE,
message = FALSE,
tidy.opts = list(width.cutoff=60),
warning = FALSE
)
# Chunk 2
library(tidyverse)
library(ggpubr)
library(rstatix)
library(readxl)
library(pastecs)
library(gmodels)
library(formatR)
# Chunk 3
nike <- read_excel("Data for assignment 8.xlsx")
# Chunk 4
nike[nike == "9"] <- NA
# Chunk 5
stat.desc(nike)
# Chunk 6
CrossTable(nike$Usage, nike$Intention)
# Chunk 7
stat.test <- nike %>%
t_test(Awareness ~ Sex) %>%
add_significance()
stat.test
# Chunk 8
stat.test1 <- nike %>%
t_test(Awareness ~ Sex, var.equal = TRUE) %>%
add_significance()
stat.test1
# Chunk 9
stat.test2 <- nike %>%
t_test(Awareness ~ Sex, var.equal = FALSE) %>%
add_significance()
stat.test2
# Chunk 10
bxp <- ggboxplot(
nike, x = "Sex", y = "Awareness",
ylab = "Awareness", xlab = "Sex", add = "jitter"
)
stat.test <- stat.test %>% add_xy_position(x = "Sex")
bxp +
stat_pvalue_manual(stat.test, tip.length = 0) +
labs(subtitle = get_test_label(stat.test, detailed = TRUE))
